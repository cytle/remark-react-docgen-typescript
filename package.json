{
  "name": "remark-react-docgen-typescript",
  "version": "0.1.6",
  "description": "remark plugin to transform React component to Markdown by react-docgen-typescript",
  "main": "build/index.js",
  "keywords": [
    "remark",
    "react-docgen-typescript",
    "react-docgen"
  ],
  "engines": {
    "node": ">= 12.13"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
