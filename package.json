    "@types/mdast": "^3.0.3",
    "@types/node": "~12.12",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "~3.9.1",
    "@typescript-eslint/parser": "~3.9.1",
    "eslint": "~7.7.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-jest": "~23.20.0",
    "eslint-plugin-react": "^7.20.6",
    "fs-extra": "^9.0.1",
    "jest": "~26.4.2",
    "prettier": "~2.0.5",
    "react": "^16.13.1",
    "remark": "^12.0.1",
    "rimraf": "~3.0.2",
    "string-width": "^4.2.0",
    "ts-jest": "~26.2.0",
    "tsutils": "~3.17.0",
    "typescript": "~4.0.2"
